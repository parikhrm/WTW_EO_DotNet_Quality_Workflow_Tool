                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">use enterprisedataservicesmi
;with cte1
as
(
select
 Hit.EntityName as [Entity Hit Name]
,Hit.ScreeningHitId  as screeningid
,Hit.ScreeningItemId as RequestDetailID
,UserAction.ActionFromName as [Action Name Raised]
,UserAction.Actiontoname as [Action Name Resolved]
,case when UserAction.ActionToId = 8 then UserAction.CreatedUTCDate else null end as QueryRaisedDate
,case when UserAction.ActionFromId = 8 and UserAction.Actiontostatusname = 'Complete' then UserAction.LastUpdatedUtcDate else null end as QueryResolvedDate
--,case when b.actionstatusid = 1 then b.Comments else null end as CorrectQueryResolvedDate
,case when UserAction.Comment like '%resolve%' then UserAction.comment else null end as CorrectQueryResolvedDate
,case when UserAction.ActionToId in (5,6) then UserAction.CreatedUTCDate else null end as ApprovalRaisedDate
,case when UserAction.ActionFromId in (5,6) then UserAction.CreatedUser else null end as ApprovalRaisedBy
,case when UserAction.ActionFromId in (5,6) and UserAction.Actiontostatusname = 'Complete' then UserAction.LastUpdatedUtcDate else null end as ApprovalReceivedDate
,case when UserAction.actiontoid is null then 'Unactioned' else 'Actioned' end as [Action/Unaction Status]
,case when UserAction.ActionToId = 7 then UserAction.CreatedUTCDate else null end as SMSOReferredDate
,case when UserAction.ActionFromId = 7 and UserAction.Actiontostatusname = 'Complete' then UserAction.ActionFromName else null end as SMSOStatus
--,case when actionraised.actionid
--pavi ,a.Events
,null as Events
,case when len(UserAction.ActionReasonName) &gt; 250 then SUBSTRING(UserAction.ActionReasonName,1,250) else UserAction.ActionReasonName end as ActionReason
,UserAction.isdeleted as [Action IsDeleted]
,R.RiskCategoryName as RiskCategoryType
,UserAction.Comment as Comments
,UserAction.ResponseReceivedDate

,convert(nvarchar(200),RC.Country) as RiskCategoryCountry
--,b.ActionReasonIds
,Hit.CreatedUTCDATE as HitCreatedUTCDate
from (select H.EntityName,H.ScreeningHitId,H.ScreeningItemId,H.CreatedUTCDATE from [kycscreening_Screening].[Hit] H with(nolock)  where H.AlertID IS NULL )  Hit 
left join (
  SELECT UA.UserActionId,UA.ScreeningHitId,UA.ActionFromId,AFrom.ActionName ActionFromName, UA.ActionToId,ATo.ActionName  Actiontoname,Astatus.ActionStatusName Actiontostatusname,UserActionReason.ActionReasonName,UA.Comment,UA.CreatedUser,UA.LastUpdatedUser, UA.CreatedUtcDate,UA.ResponseReceivedDate,Ua.LastUpdatedUtcDate,UA.IsDeleted
  FROM [EnterpriseDataServicesMI].[kycscreening_Screening].[UserAction] UA with(nolock)
  LEFT JOIN (SELECT UserActionId, ActionReasonName = 
               STUFF((SELECT ', ' + (SELECT AR.ActionReasonName FROM [EnterpriseDataServicesMI].[kycscreening_Ref].[ActionReason] AR with(nolock) WHERE AR.ActionReasonId = ReasonId)
               FROM [kycscreening_Screening].[UserActionReason] b with(nolock)
               WHERE b.UserActionId = a.UserActionId
               FOR XML PATH('')), 1, 2, '')
               FROM [kycscreening_Screening].[UserActionReason] a with(nolock)
               GROUP BY UserActionId) UserActionReason ON UserActionReason.UserActionId = UA.UserActionId 
  LEFT JOIN [EnterpriseDataServicesMI].[kycscreening_Ref].[Action] ATo with(nolock) ON ATo.ActionId = UA.ActionToId 
  LEFT JOIN [EnterpriseDataServicesMI].[kycscreening_Ref].[Action] AFrom with(nolock) ON AFrom.ActionId = UA.ActionFromId 
  LEFT JOIN [kycscreening_Ref].[ActionStatus] Astatus with(nolock) ON Astatus.ActionStatusid = ATo.ActionStatusId 
) UserAction on UserAction.ScreeningHitId = Hit.ScreeningHitId

left join (select HRC.ScreeningHitId,HRC.Country,HRC.HitRiskCategoryId,HRC.RiskCategoryId from [kycscreening_Screening].[HitRiskCategory] HRC with(nolock) where IsDeleted = 0) RC on Hit.ScreeningHitId = RC.ScreeningHitId
left join (select R.RiskCategoryId,R.RiskCategoryName from [kycscreening_Ref].[RiskCategory] R with(nolock)) R on R.RiskCategoryId = RC.RiskCategoryId
where 1=1  
and (UserAction.ActionFromName = 'Refer-SMSO' or UserAction.Actiontoname = 'Refer-SMSO')
)
select
 Hit.EntityName as [Entity Hit Name]
,Request.Requestid
,Request.RequestName as PartyName
,Principal.Name as PrincipalName
,Hit.ScreeningHitId  as screeningid
,Hit.ScreeningItemId as RequestDetailID
,UserAction.ActionFromName as [Action Name Raised]
,UserAction.Actiontoname as [Action Name Resolved]
,case when UserAction.ActionToId = 7 then UserAction.CreatedUTCDate else null end as SMSOReferredDate
,case when len(UserAction.ActionReasonName) &gt; 250 then SUBSTRING(UserAction.ActionReasonName,1,250) else UserAction.ActionReasonName end as ActionReason
,R.RiskCategoryName as RiskCategoryType
,UserAction.Comment as Comments
,UserAction.ResponseReceivedDate
,GetDate() as LastUpdatedDateTime
from (select H.EntityName,H.ScreeningHitId,H.ScreeningItemId,H.CreatedUTCDATE from [kycscreening_Screening].[Hit] H with(nolock)  where H.AlertID IS NULL )  Hit 
left join (
  SELECT UA.UserActionId,UA.ScreeningHitId,UA.ActionFromId,AFrom.ActionName ActionFromName, UA.ActionToId,ATo.ActionName  Actiontoname,Astatus.ActionStatusName Actiontostatusname,UserActionReason.ActionReasonName,UA.Comment,UA.CreatedUser,UA.LastUpdatedUser, UA.CreatedUtcDate,UA.ResponseReceivedDate,Ua.LastUpdatedUtcDate,UA.IsDeleted
  FROM [EnterpriseDataServicesMI].[kycscreening_Screening].[UserAction] UA with(nolock)
  LEFT JOIN (SELECT UserActionId, ActionReasonName = 
               STUFF((SELECT ', ' + (SELECT AR.ActionReasonName FROM [EnterpriseDataServicesMI].[kycscreening_Ref].[ActionReason] AR with(nolock) WHERE AR.ActionReasonId = ReasonId)
               FROM [kycscreening_Screening].[UserActionReason] b with(nolock)
               WHERE b.UserActionId = a.UserActionId
               FOR XML PATH('')), 1, 2, '')
               FROM [kycscreening_Screening].[UserActionReason] a with(nolock)
               GROUP BY UserActionId) UserActionReason ON UserActionReason.UserActionId = UA.UserActionId 
  LEFT JOIN [EnterpriseDataServicesMI].[kycscreening_Ref].[Action] ATo with(nolock) ON ATo.ActionId = UA.ActionToId 
  LEFT JOIN [EnterpriseDataServicesMI].[kycscreening_Ref].[Action] AFrom with(nolock) ON AFrom.ActionId = UA.ActionFromId 
  LEFT JOIN [kycscreening_Ref].[ActionStatus] Astatus with(nolock) ON Astatus.ActionStatusid = ATo.ActionStatusId 
) UserAction on UserAction.ScreeningHitId = Hit.ScreeningHitId

left join (select HRC.ScreeningHitId,HRC.Country,HRC.HitRiskCategoryId,HRC.RiskCategoryId from [kycscreening_Screening].[HitRiskCategory] HRC with(nolock) where IsDeleted = 0) RC on Hit.ScreeningHitId = RC.ScreeningHitId
left join (select R.RiskCategoryId,R.RiskCategoryName from [kycscreening_Ref].[RiskCategory] R with(nolock)) R on R.RiskCategoryId = RC.RiskCategoryId
left join (select * from [kycscreening_Screening].[Item]  with(nolock) where 1=1 /*isdeleted = 0*/ and IsDuplicate = 0 and ClientID is not null) Principal  on principal.ScreeningItemId = Hit.ScreeningItemId
left join 
		(SELECT  I.*
		        ,SegmentId as SegmentCode
		        ,BD.LineOfBusinessId as LobCode
		        ,BD.LegalEntityId as LegalEntityCode
		        ,BD.JurisdictionId as JurisdictionCode
		 from [kycRequest_Request].[Item] I with(nolock) INNER JOIN [kycrequest_Request].[BusinessDetail] BD with(nolock) ON BD.RequestId = I.RequestId   where isdeleted = 0 ) Request on Request.ClientId = Principal.ClientId
left join (select * from [kycrequest_Ref].[Source] with(nolock) where isdeleted = 0) RequestSource on Request.SourceID = RequestSource.sourceID
where 1=1  
and Hit.ScreeningItemId in (select distinct RequestDetailID from cte1)
--and RequestSource.SourceName not in ('Bulk','Monitoring','Screening')
and Request.Requestid is not null
order by Hit.ScreeningItemId, UserAction.CreatedUtcDate</property>
